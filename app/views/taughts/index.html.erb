<p id="notice"><%= notice %></p>

<h1>Taughts</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Instructorr</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @taughts.each do |taught| %>
      <tr>
        <td><%= taught.course %></td>
        <td><%= taught.instructorr %></td>
        <td><%= link_to 'Show', taught %></td>
        <td><%= link_to 'Edit', edit_taught_path(taught) %></td>
        <td><%= link_to 'Destroy', taught, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Taught', new_taught_path %>
